<h1>Dashboard</h1>
<h3>Welcome <%= current_user.email %></h3>
<%= link_to 'Update Details', edit_user_registration_path %>

<h2>My Events</h2>
<% @events.each do |event| %>
  <p><%= event.name %></p>
<% end %>
<h2>My Attendances</h2>
<% @attendances.each do |attendance| %>
  <p><%= attendance.event.name %></p>
<% end %>
